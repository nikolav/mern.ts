{
  "name": "reactapp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "node ./serve/www",
    "dev": "concurrently \"yarn run tw:watch\" \"vite --host\" ",
    "build": "yarn run tw:build && tsc && vite build",
    "lint": "eslint ./src",
    "test": "vitest ",
    "format": "yarn prettier --write src ",
    "preview": "vite preview",
    "test:1": "vitest run ",
    "test:coverage": "yarn run test:1 && yarn run coverage ",
    "test:ui": "vitest --ui ",
    "coverage": "vitest run --coverage ",
    "tw:watch": "tailwindcss -i ./src/tailwind.css -o ./src/styles/tw-build.css --watch",
    "tw:build": "tailwindcss -i ./src/tailwind.css -o ./src/styles/tw-build.css "
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.11",
    "@reduxjs/toolkit": "^1.8.6",
    "axios": "^1.1.3",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "event-emitter": "^0.3.5",
    "express": "~4.16.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^7.6.2",
    "graphql": "^16.6.0",
    "jquery": "^3.6.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.38.0",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-use": "^17.4.0",
    "rxjs": "^7.5.7",
    "socket.io-client": "^4.5.3",
    "validator": "^13.7.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/dom": "^8.19.0",
